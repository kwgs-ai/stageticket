<% @page_title = "承認あと一覧" %>
<h1><%= @page_title %></h1>


<% if @stages.present? %>
<%= render "shared/table_list", stages: @stages %>
<% end %>
<%= paginate @stages %>

<% if @after.present? %>
  <h1>過去</h1>
<%= render "shared/table_list", stages: @after %>
<% end %>
<%= paginate @after %>
<% if @stages %>
  <%= render "shared/table_list", stages: @stages %>
<% end %>

